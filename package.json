{
  "name": "n8n-nodes-layerre",
  "version": "1.0.0",
  "description": "n8n community node for Layerre - create templates and variants from Canva designs",
  "license": "MIT",
  "homepage": "https://layerre.com",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Layerre",
    "email": "hello@layerre.com",
    "url": "https://layerre.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/layerre/n8n-nodes-layerre.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/LayerreApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Layerre/Layerre.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
